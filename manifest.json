{
"manifest_version": 2,
    "name": "Smartling Chrome - Mask Staging as Production",
    "description": "Make Staging look like Prod in the Smartling Dashboard",
    "version": "0.1",
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}